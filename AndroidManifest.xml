    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.domandroid.ActValores"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name="com.example.domandroid.ActModul"
        	android:label="@string/frmModul">
        </activity>
        <activity android:name="com.example.domandroid.ActConfig"
        	android:label="@string/frmConfig">
        </activity>
        <activity android:name="com.example.domandroid.ActConfigMod"
        	android:label="@string/frmConfigMod">
        </activity>
    </application>

</manifest>
